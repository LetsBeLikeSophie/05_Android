<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="a.b.c.com.login.dao.SpringLoginDAO">


<select id="loginCheck" parameterType="memberVO" resultType="memberVO">	
	SELECT	 A.MNUM 			MNUM 
			,A.MNAME 			MNAME 
			,A.MID 				MID 
			,A.MPW 				MPW 
            ,A.MGENDER 			MGENDER    
            ,A.MBIRTH 			MBIRTH 
            ,A.MHP 				MHP 
            ,A.MTEL 			MTEL
            ,A.MEMAIL 			MEMAIL   
            ,A.MZONECODE 		MZONECODE  
            ,A.MROADADDRESS 	MROADADDRESS 
            ,A.MJIBUNADDRESS 	MJIBUNADDRESS 
            ,A.MHOBBY 			MHOBBY   
            ,A.MINFO 			MINFO
            ,A.MPHOTO 			MPHOTO
            ,A.MADMIN 			MADMIN 
            ,A.DELETEYN 		DELETEYN
            ,TO_CHAR(A.INSERTDATE, 'YYYY-MM-DD')  INSERTDATE 
            ,TO_CHAR(A.UPDATEDATE, 'YYYY-MM-DD')  UPDATEDATE     
	FROM 	SPRING_MEMBER A
	WHERE 	A.DELETEYN = 'Y' 	
	ORDER BY 1 DESC  
</select>


</mapper>

